<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="541.0" prefWidth="891.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.SecondaryController">
    <children>
      <HBox prefHeight="414.0" prefWidth="851.0">
         <children>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <ImageView fx:id="img1" fitHeight="400.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <ImageView fx:id="img2" fitHeight="400.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <ImageView fx:id="img3" fitHeight="400.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <Separator prefWidth="200.0" />
      <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" />
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
         <children>
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="BACK" />
            <Button fx:id="btn_reset" mnemonicParsing="false" text="RESET" />
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
